# Wallet Connect
NEXT_PUBLIC_WC_PROJECT_ID=your_walletconnect_project_id

# Smart Contract
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Alchemy (for RPC)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key

# Solana Network & RPC (for Solana wallets)
# Options: mainnet-beta | devnet | testnet
NEXT_PUBLIC_SOLANA_NETWORK=devnet
# Optional: custom RPC endpoint. If empty, uses clusterApiUrl(network)
NEXT_PUBLIC_SOLANA_RPC_URL=

# SPL Token mints (optional)
# Set these to filter balances for USDC/USDT or other tokens
# Example mainnet:
# NEXT_PUBLIC_USDC_MINT=EPjFWdd5AufqSSqeM2q8XxhipvYMR2iNrDrS1n1fKX6
# NEXT_PUBLIC_USDT_MINT=Es9vMFrzaCERZzY7zGd6W9Y7RrkY3gNzF2KphcGZ8k2
NEXT_PUBLIC_USDC_MINT=
NEXT_PUBLIC_USDT_MINT=

# Database (Optional - for production)
DATABASE_URL=postgresql://username:password@host:port/database

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# API Keys for content crawling
CRAWLER_API_KEY=your_crawler_api_key

# Environment
NODE_ENV=development